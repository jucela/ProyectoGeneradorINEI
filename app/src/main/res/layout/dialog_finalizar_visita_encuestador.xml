<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/dialog_finalizar_visita_lyt"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:padding="10dp">
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/greyBluePrimaryDark">
            <TextView
                android:id="@+id/dialog_finalizar_visita_txtNumero"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:gravity="center"
                android:textColor="@color/colorIcons"
                android:textStyle="bold"
                android:text="VISITA N° 01"/>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margen_small">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textColor="@color/greyBluePrimaryDark"
                    android:gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:text="HORA FIN"
                    android:textSize="15sp"/>
                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="30dp">
                    <TextView
                        android:id="@+id/dialog_finalizar_visita_txtHoraFin"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"/>
                </android.support.v7.widget.CardView>
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margen_small">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textColor="@color/greyBluePrimaryDark"
                    android:gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:text="RESULTADO"/>
                <android.support.v7.widget.CardView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="30dp">
                    <Spinner
                        android:id="@+id/dialog_finalizar_visita_spResultado"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:drawSelectorOnTop="true"
                        android:gravity="center_vertical"
                        android:entries="@array/visita_array_resultados"
                        />
                </android.support.v7.widget.CardView>
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView
            android:id="@+id/dialog_cardview_finalizar_especifique"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margen_small">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:textColor="@color/greyBluePrimaryDark"
                    android:gravity="center_vertical"
                    android:layout_height="match_parent"
                    android:text="ESPECIFIQUE"/>
                <EditText
                    android:id="@+id/dialog_finalizar_visita_edtEspecifique"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="24dp"
                    android:gravity="center_vertical"
                    android:inputType="text"
                    android:paddingLeft="@dimen/margen_small"
                    android:background="@drawable/input_text_enabled"
                    android:enabled="false"/>
            </LinearLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp">
                <CheckBox
                    android:id="@+id/dialog_finalizar_visita_ckProximaVisita"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ASIGNAR PRÓXIMA VISITA"
                    android:checked="false"
                    android:textColor="@color/colorPrimaryDark"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/margen_small">
                        <TextView
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimaryDark"
                            android:gravity="center_vertical"
                            android:layout_height="match_parent"
                            android:text="FECHA PRÓXIMA VISITA"/>
                        <android.support.v7.widget.CardView
                            android:id="@+id/dialog_cardview_proxFecha"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="30dp"
                            app:cardBackgroundColor="@color/greyPrimary">
                            <TextView
                                android:id="@+id/dialog_finalizar_visita_txtFechaProx"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:enabled="false"/>
                        </android.support.v7.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/margen_small"
                        >
                        <TextView
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:textColor="@color/colorPrimaryDark"
                            android:gravity="center_vertical"
                            android:layout_height="match_parent"
                            android:text="HORA PRÓXIMA VISITA"/>
                        <android.support.v7.widget.CardView
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="30dp"
                            android:id="@+id/dialog_cardview_proxHora"
                            app:cardBackgroundColor="@color/greyPrimary">
                            <TextView
                                android:id="@+id/dialog_finalizar_visita_txtHoraProx"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:enabled="false"/>
                        </android.support.v7.widget.CardView>
                    </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>

        </LinearLayout>
</LinearLayout>